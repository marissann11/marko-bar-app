import seedData from "../../../seed.json"
import getDollarAmount from "../../../utils/helpers.js"

class {
  onCreate() {
    let autoGrat = 0;
    seedData.tabs.forEach(el => {
      autoGrat += el.autogratuity
    })
    return `${getDollarAmount(autoGrat)}`
  }
}

<div class="col-12 col-md-3" style="width: 18rem;">
  <div class="card-body">
    <h5 class="card-title">Automatic Gratuity</h5>
    <p class="card-text">${component.onCreate()}</p>
  </div>
</div>